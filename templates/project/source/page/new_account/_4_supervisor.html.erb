<div class="row top-buffer">
	<ol class="breadcrumb">
		<li class="active"><a href='javascript:;'>平台管理</a></li>
		<li class="active"><a href='javascript:;'>帳號設定</a></li>
		<li class="active"><a href='javascript:;'>行政人員</a></li>
		<li class="active">新增帳號</li>
	</ol>
</div>
<form class="row top-buffer">
	<div class="text-right">
		<input class="btn btn-default" type="button" value="取消">
		<input class="btn btn-default" type="submit" value="送出">
	</div>
	<hr>
	<div class="row">
		<div class="col-md-2 col-md-offset-2">
			<div class="form-group text-center">
				<img alt="" class="img-responsive img-thumbnail" src="http://placehold.it/200">
				<div class="help-block top-buffer">選擇相片
					<br>
					<input type="file" href="javascript:;">
				</div>
			</div>
		</div>

		<div class="col-md-6 form-horizontal">

			<div class="form-group">
				<label class="col-md-4 control-label">啟用狀態</label>
				<div class="col-md-8">
					<label class="radio-inline">
						<input type="radio" value="" name="EnableRadios" checked="">啟用</label>
					<label class="radio-inline">
						<input type="radio" value="" name="EnableRadios">停用</label>
				</div>
			</div>
			<div class="form-group">
				<label for="" class="col-md-4 control-label">Email</label>
				<div class="col-md-8">
					<input type="email" class="form-control" placeholder="">
				</div>
			</div>
			<div class="form-group">
				<label for="" class="col-md-4 control-label">密碼</label>
				<div class="col-md-8">
					<input type="password" class="form-control" placeholder="">
				</div>
			</div>
			<div class="form-group">
				<label for="" class="col-md-4 control-label">姓名</label>
				<div class="col-sm-8">
					<input type="text" class="form-control" placeholder="">
				</div>
			</div>
			<div class="form-group">
				<label for="" class="col-md-4 control-label">聯絡電話</label>
				<div class="col-md-8">
					<input type="text" class="form-control" placeholder="">
				</div>
			</div>
			<div class="form-group">
				<label for="" class="col-md-4 control-label">隸屬主管</label>
				<div class="col-md-8">
					<input type="text" class="form-control" placeholder="">
				</div>
			</div>
		
		</div>
	</div>
	<hr>
	<div class="row form-horizontal">
		<div class="form-group">
			<label class="col-md-2 control-label">所屬服務員</label>
			<div class="col-md-10">
				<div class="checkbox">
					<label>
						<input type="checkbox" value=""> 全選</label>
				</div>
				<% 20.times do %>
					<div class="col-md-2"
						<label class="checkbox-inline">
							<input type="checkbox" value=""> 張某某</label>
					</div>
				<% end %>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label">服務個案</label>
			<div class="col-md-10">
				<div class="checkbox">
					<label>
						<input type="checkbox" value=""> 全選</label>
				</div>
				<% 20.times do %>
					<div class="col-md-2"
						<label class="checkbox-inline">
							<input type="checkbox" value=""> 張某某</label>
					</div>
				<% end %>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label">身分別(一)</label>
			<div class="col-md-3">
				<select class="form-control">
					<option value=""></option>
					<option value="">一般民眾</option>
					<option value="">原住民</option>
					<option value="">低收入戶</option>
					<option value="">其他</option>
				</select>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label">身分別(二)</label>
			<div class="col-md-3">
				<select class="form-control">
					<option value=""></option>
					<option value="">中高齡</option>
					<option value="">非中高齡</option>
				</select>
			</div>
		</div>
	</div>
</form>